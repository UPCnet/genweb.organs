  <script type="text/javascript">
    $(function() {
      $('.expand').hide()
      $('.expand').click(function(e){
              e.stopPropagation();
              $(this).hide();
              $(this).siblings('.notexpand').show();
              $(this).parent().parent().siblings('.sortable2').slideDown();
              $(this).parent().parent().parent().find('.listFiles').slideDown();
      });

      $('.notexpand').click(function(e){
              e.stopPropagation();
              $(this).hide();
              $(this).siblings('.expand').show();
              $(this).parent().parent().siblings('.sortable2').slideUp();
              $(this).parent().parent().parent().find('.listFiles').slideUp();
      });
      $("#expandAll").hide()
      $("#expandAll").click(function(){
        $(".expand").click();
        $("#expandAll").hide();
        $("#collapseAll").show();
      });

      $("#collapseAll").click(function(){
        $(".notexpand").click();
        $("#expandAll").show();
        $("#collapseAll").hide();
      });

      $('.send-sign').click(function(e){$('#send-confirmation').modal('show');})

    });
  </script>

