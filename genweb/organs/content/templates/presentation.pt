<metal:content-core tal:define="items view/PuntsInside">

<ul id="navbarEpd" class="affix nav" style="z-index=2; right:0; top 170px;">
<li>
                        <a href="" title="Close" data-toggle="tooltip" data-container="body" data-placement="left" data-original-title="Close">
