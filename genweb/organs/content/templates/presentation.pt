<metal:page define-macro="master"><tal:doctype tal:replace="structure string:&lt;!DOCTYPE html&gt;" />

<tal:maindefines
    define="portal_state context/@@plone_portal_state;
            context_state context/@@plone_context_state;
            plone_view context/@@plone;
            genweb_config context/@@gwconfig;
            lang portal_state/language;
            view nocall:view | nocall: plone_view;
            dummy python: plone_view.mark_view(view);
            portal_url portal_state/portal_url;
            checkPermission nocall: context/portal_membership/checkPermission;
            site_properties context/portal_properties/site_properties;
            ajax_load request/ajax_load | nothing;
            ajax_include_head request/ajax_include_head | nothing;
            dummy python:request.RESPONSE.setHeader('X-UA-Compatible', 'IE=EmulateIE10');
            dummy python:request.RESPONSE.setHeader('X-UA-Compatible', 'IE=edge,chrome=1,requiresActiveX=true');"
            >
<tal:IElt7 tal:replace="structure string:&lt;!--[if lt IE 7]&gt; &lt;html xmlns='http://www.w3.org/1999/xhtml' class='no-js lt-ie10 lt-ie9 lt-ie8 lt-ie7' lang=${lang}&gt; &lt;![endif]--&gt;" />
<tal:IE7 tal:replace="structure string:&lt;!--[if IE 7]&gt;    &lt;html xmlns='http://www.w3.org/1999/xhtml' class='no-js lt-ie10 lt-ie9 lt-ie8' lang=${lang}&gt; &lt;![endif]--&gt;" />
<tal:IE8 tal:replace="structure string:&lt;!--[if IE 8]&gt;    &lt;html xmlns='http://www.w3.org/1999/xhtml' class='no-js lt-ie10 lt-ie9' lang=${lang}&gt; &lt;![endif]--&gt;" />
<tal:IE9 tal:replace="structure string:&lt;!--[if IE 9]&gt;    &lt;html xmlns='http://www.w3.org/1999/xhtml' class='no-js lt-ie10' lang=${lang}&gt; &lt;![endif]--&gt;" />
<!--[if gt IE 9]><!-->
<html xmlns="http://www.w3.org/1999/xhtml"
      class="no-js"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="plone"
      tal:attributes="lang lang;"
      >
<!--<![endif]-->

    <metal:cache use-macro="context/global_cache_settings/macros/cacheheaders">
    Get the global cache headers located in global_cache_settings.
    </metal:cache>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <tal:comment replace="nothing">  <!-- EMulateIE10 sirve para corregir bug TINY crear links  --> </tal:comment>
    <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE10">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1,requiresActiveX=true" />
    <meta name="generator" content="Plone - http://plone.org" />
    <meta name="author" tal:attributes="content genweb_config/meta_author | nothing" />
    <link rel="shortcut icon" type="image/x-icon" tal:attributes="href string:${portal_url}/favicon.ico" />

    <metal:baseslot define-slot="base">
        <base tal:attributes="href plone_view/renderBase" /><!--[if lt IE 7]></base><![endif]-->
    </metal:baseslot>

    <tal:notajax tal:condition="python:not ajax_load or ajax_include_head">
        <div tal:replace="structure provider:plone.htmlhead" />

        <tal:comment replace="nothing">
            Various slots where you can insert elements in the header from a template.
        </tal:comment>
        <metal:topslot define-slot="top_slot" />
        <metal:headslot define-slot="head_slot" />
        <metal:styleslot define-slot="style_slot" />

        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <tal:comment replace="nothing"><!-- El seguent script ha d'anar seguit del meta del viewport --></tal:comment>
        <script tal:attributes="src string:${portal_url}/++genweb++static/js/ReView.js"></script>

        <link rel="apple-touch-icon" tal:attributes="href string:${portal_url}/apple-touch-icon.png" /> <tal:comment replace="nothing"> <!-- 60x60 --> </tal:comment>
        <link rel="apple-touch-icon" sizes="57x57" tal:attributes="href string:${portal_url}/apple-touch-icon-57x57-precomposed.png" /> <tal:comment replace="nothing">  <!-- iPhone --> </tal:comment>
        <link rel="apple-touch-icon" sizes="72x72" tal:attributes="href string:${portal_url}/apple-touch-icon-72x72-precomposed.png" /> <tal:comment replace="nothing">  <!-- iPad --> </tal:comment>
        <link rel="apple-touch-icon" sizes="114x114" tal:attributes="href string:${portal_url}/apple-touch-icon-114x114-precomposed.png" /> <tal:comment replace="nothing">  <!-- iPhone 2x --> </tal:comment>
        <link rel="apple-touch-icon" sizes="144x144" tal:attributes="href string:${portal_url}/apple-touch-icon-144x144-precomposed.png" /> <tal:comment replace="nothing"> <!-- iPad 2x --> </tal:comment>

        <div tal:replace="structure provider:genweb.css" />

        <tal:comment content="nothing"> <!-- Splitting CSS only for IE, the rest the common CSS --> </tal:comment>
        <!--[if !IE]><!-->
          <link rel="stylesheet" tal:attributes="href string:${portal_url}/++genweb++stylesheets/genwebupc.css">
        <!--<![endif]-->

        <tal:splittedCSS4IE tal:replace="structure string:&lt;!--[if lt IE 10]&gt;&lt;link rel='stylesheet' href='${portal_url}/++genweb++stylesheets/genwebupc-ie.css'&gt;&lt;![endif]--&gt;" />

        <link rel="stylesheet" tal:attributes="href string:${portal_url}/dynamic.css">

    </tal:notajax>
</head>
<body style="padding-top:50px; padding-bottom: 50px; background-color: beige;" id="home">

         <link rel="stylesheet" href="//fontawesome.io/assets/font-awesome/css/font-awesome.css">




<metal:content-core tal:define="items view/PuntsInside">

<ul id="navbarEpd" class="affix nav" style="z-index=2; right:0; top 170px;">
<li>
                        <a href="" title="Close" data-toggle="tooltip" data-container="body" data-placement="left" data-original-title="Close">
                            <i class="fa fa-close fa-3x" aria-hidden="true"></i></a>
                    </li><li>
                        <a href="presentation#home" title="" data-toggle="tooltip" data-container="body" data-placement="left" data-original-title="Home">
                            <i class="fa fa-chevron-up fa-3x" aria-hidden="true"></i></a>
                    </li>

                    <li>
                        <a href="presentation#end" title="" data-toggle="tooltip" data-container="body" data-placement="left" data-original-title="End">
                            <i class="fa fa-chevron-down fa-3x" aria-hidden="true"></i></a>
                    </li>
                </ul>

<div tal:condition="items" class="DADES"  
     style="margin-right: auto; margin-left: auto; width: 978px; background-color: white;">
    <h1 tal:content="view/getSessionTitle" style='text-transform: uppercase'>Ordre del dia</h1>
    <hr/>
    <ul id="sortable" class="ui-sortable">
      <tal:documents repeat="item items">
        <li>
            <div class="puntTitle" tal:define="agreement item/agreement">
              <span class="proposalNumber" tal:content="item/proposalPoint"></span>
              <span class="agreementClass" tal:condition="agreement" tal:content="agreement"></span>
              <a tal:attributes="href string:${item/absolute_url}/view">
                <tal:omit-tag tal:content="item/title">Item Title</tal:omit-tag>
              </a>
                <div class="btn-group" role="group"
                   style="float: right;">
                <tal:block tal:condition="item/state">
                  <button class="label dropdown-toggle"
                      id="estats" type="button" data-toggle="dropdown"
                      aria-haspopup="true" aria-expanded="false">
                      <i class="fa fa-circle" aria-hidden="true" tal:attributes="style string:color:${item/css}"></i>
                    <a tal:content="item/state"></a>
                    <span class="caret"></span>
                  </button>
                  <ul class="dropdown-menu" role="menu" aria-labelledby="estats">
                    <tal:block repeat="estat item/estats">
                      <li><a tabindex="-1"><i class="fa fa-circle" aria-hidden="true" tal:attributes="style string:color:${estat/color}"></i>&nbsp;<span tal:content="estat/title"></span></a></li>
                    </tal:block>
                  </ul>
                </tal:block>
              </div>
              <tal:files tal:define="files python:view.filesinside(item)" tal:condition="files">
                <ul style="list-style: none;">
                  <tal:documents repeat="item files">
                    <li class="filesinTable">
                      <i tal:attributes=" class item/classCSS"></i>
                      <a tal:attributes="href item/absolute_url;">
                         <tal:omit-tag tal:content="item/title"> Doc title</tal:omit-tag>
                      </a>
                    </li>
                  </tal:documents>
                </ul>
              </tal:files>
            </div>
            <div tal:define="subpunts python:view.SubpuntsInside(item)" tal:condition="subpunts">
              <tal:subpunts repeat="item subpunts">
                <ol class="li_subgrups">
                  <td>
                    <div class="subpuntSession" tal:define="agreement item/agreement">
                      <span class="proposalNumberSubpunt" tal:content="item/proposalPoint"></span>
                      <span class="agreementClass" tal:condition="agreement" tal:content="agreement"></span>
                      <a tal:attributes="href string:${item/absolute_url}"
                         class="">
                        <tal:omit-tag tal:content="item/title">Item Title</tal:omit-tag>
                      </a>
                      <div class="btn-group" role="group"
                           style="float: right;" aria-label="Subpunt actions">
                        <tal:block tal:condition="item/state">
                          <button class="label dropdown-toggle"
                              id="estats" type="button" data-toggle="dropdown"
                              aria-haspopup="true" aria-expanded="true">
                              <i class="fa fa-circle" aria-hidden="true" tal:attributes="style string:color:${item/css}"></i>
                            <a tal:content="item/state"></a>
                            <span class="caret"></span>
                          </button>
                          <ul class="dropdown-menu" role="menu" aria-labelledby="estats">
                            <tal:block repeat="estat item/estats">
                              <li><a tabindex="-1"><i class="fa fa-circle" aria-hidden="true" tal:attributes="style string:color:${estat/color}"></i>&nbsp;<span tal:content="estat/title"></span></a></li>
                            </tal:block>
                          </ul>
                        </tal:block>
                      </div>
                      <tal:files tal:define="files python:view.filesinside(item)" tal:condition="files">
                        <ul style="list-style: none;">
                          <tal:documents repeat="item files">
                            <li class="filesinTable">
                              <i tal:attributes=" class item/classCSS"></i>
                              <a tal:attributes="href item/absolute_url;">
                                 <tal:omit-tag tal:content="item/title"> Doc title</tal:omit-tag>
                              </a>
                            </li>
                          </tal:documents>
                        </ul>
                      </tal:files>
                    </div>
                  </td>
                </ol>
              </tal:subpunts>
            </div>
        </li>
      </tal:documents>
    </ul>
  </div>

  <div style="margin-bottom:20px;" id="end"></div>


</metal:content-core>

</body>
</html>

</tal:maindefines>

</metal:page>
