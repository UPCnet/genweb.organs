<metal:page define-macro="master" tal:condition="view/canView">
<tal:doctype tal:replace="structure string:&lt;!DOCTYPE html&gt;" />

<tal:maindefines
    define="portal_state context/@@plone_portal_state;
            lang portal_state/language;
            portal_url portal_state/portal_url;
            plone_view context/@@plone;
            ajax_load request/ajax_load | nothing;">

<tal:IElt7 tal:replace="structure string:&lt;!--[if lt IE 7]&gt; &lt;html xmlns='http://www.w3.org/1999/xhtml' class='no-js lt-ie10 lt-ie9 lt-ie8 lt-ie7' lang=${lang}&gt; &lt;![endif]--&gt;" />
<tal:IE7 tal:replace="structure string:&lt;!--[if IE 7]&gt;    &lt;html xmlns='http://www.w3.org/1999/xhtml' class='no-js lt-ie10 lt-ie9 lt-ie8' lang=${lang}&gt; &lt;![endif]--&gt;" />
<tal:IE8 tal:replace="structure string:&lt;!--[if IE 8]&gt;    &lt;html xmlns='http://www.w3.org/1999/xhtml' class='no-js lt-ie10 lt-ie9' lang=${lang}&gt; &lt;![endif]--&gt;" />
<tal:IE9 tal:replace="structure string:&lt;!--[if IE 9]&gt;    &lt;html xmlns='http://www.w3.org/1999/xhtml' class='no-js lt-ie10' lang=${lang}&gt; &lt;![endif]--&gt;" />
<!--[if gt IE 9]><!-->
<html xmlns="http://www.w3.org/1999/xhtml"
      class="no-js"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="genweb.organs"
      tal:attributes="lang lang;"
      >
<!--<![endif]-->

    <metal:cache use-macro="context/global_cache_settings/macros/cacheheaders">
    Get the global cache headers located in global_cache_settings.
    </metal:cache>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <tal:comment replace="nothing">  </tal:comment>
    <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE10">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1,requiresActiveX=true" />
    <meta name="generator" content="Plone - http://plone.org" />
    <meta name="author" tal:attributes="content genweb_config/meta_author | nothing" />
    <link rel="shortcut icon" type="image/x-icon" tal:attributes="href string:${portal_url}/favicon.ico" />

    <metal:baseslot define-slot="base">
        <base tal:attributes="href plone_view/renderBase" /><!--[if lt IE 7]></base><![endif]-->
    </metal:baseslot>

    <tal:notajax tal:condition="python:not ajax_load or ajax_include_head">
        <div tal:replace="structure provider:plone.htmlhead" />

        <tal:comment replace="nothing">
            Various slots where you can insert elements in the header from a template.
        </tal:comment>
        <metal:topslot define-slot="top_slot" />
        <metal:headslot define-slot="head_slot" />
        <metal:styleslot define-slot="style_slot" />

        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <tal:comment replace="nothing"></tal:comment>

        <link rel="apple-touch-icon" tal:attributes="href string:${portal_url}/apple-touch-icon.png" /> <tal:comment replace="nothing">  </tal:comment>
        <link rel="apple-touch-icon" sizes="57x57" tal:attributes="href string:${portal_url}/apple-touch-icon-57x57-precomposed.png" /> <tal:comment replace="nothing"></tal:comment>
        <link rel="apple-touch-icon" sizes="72x72" tal:attributes="href string:${portal_url}/apple-touch-icon-72x72-precomposed.png" /> <tal:comment replace="nothing"></tal:comment>
        <link rel="apple-touch-icon" sizes="114x114" tal:attributes="href string:${portal_url}/apple-touch-icon-114x114-precomposed.png" /> <tal:comment replace="nothing"></tal:comment>
        <link rel="apple-touch-icon" sizes="144x144" tal:attributes="href string:${portal_url}/apple-touch-icon-144x144-precomposed.png" /> <tal:comment replace="nothing"></tal:comment>

        <div tal:replace="structure provider:genweb.css" />

        <tal:comment content="nothing"></tal:comment>
        <!--[if !IE]><!-->
          <link rel="stylesheet" tal:attributes="href string:${portal_url}/++genweb++stylesheets/genwebupc.css">
        <!--<![endif]-->
        <link rel="stylesheet" tal:attributes="href string:${portal_url}/++resource++genweb.organs.stylesheets/genweb_organs.css" />

        <tal:splittedCSS4IE tal:replace="structure string:&lt;!--[if lt IE 10]&gt;&lt;link rel='stylesheet' href='${portal_url}/++genweb++stylesheets/genwebupc-ie.css'&gt;&lt;![endif]--&gt;" />
        <link rel="stylesheet" tal:attributes="href string:${portal_url}/dynamic.css">

    </tal:notajax>
</head> 
<tal:block i18n:domain="genweb.organs"
           tal:define="getActaLogo view/getActaLogo;
                      signatura view/signatura;
                      Title context/Title;
                      llocConvocatoria context/llocConvocatoria;
                      horaInici context/horaInici;
                      horaFi context/horaFi;
                      membresConvocats context/membresConvocats;
                      membresConvidats context/membresConvidats;
                      llistaExcusats context/llistaExcusats;
                      noAssistents context/noAssistents;
                      ordenDelDia context/ordenDelDia">

    <link href='//fonts.googleapis.com/css?family=Source+Sans+Pro:400,733,600' rel='stylesheet' type='text/css'>
    <link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />

    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap-theme.min.css">
    <link rel="stylesheet" href="++genwebupc++stylesheets/genwebupc.css">
    <link rel="stylesheet" href="++resource++genweb.organs.stylesheets/genweb_organs.css" />
    <link rel="stylesheet" media="print" href="++resource++genweb.organs.stylesheets/print.css">

    <div class="clearfix" style="width:180px; float:right;">
      <img tal:condition="getActaLogo" tal:attributes="src getActaLogo" />
    </div>
    <br/>
    <h1 style="text-align: left"><span tal:content="view/unitatTitle">UNITAT</span> -
    <span tal:content="view/organGovernTitle">CONSELL DE GOVERN</span></h1>
    <h2 i18n:translate=""> ACTA <span style="text-align: left" tal:content="view/sessionTitle">CONSELL DE GOVERN</span></h2>
    <hr/>

    <div class="container-fluid" style="border-bottom: 1px solid #b2b2b2;">
      <div class="row-fluid">
        <div class="span12" style="margin-bottom: 0px">
          <div class="row-fluid">
              <tal:omit-tag tal:condition="llocConvocatoria">
                <th style="display: inline-block;"><strong i18n:translate="">Session location</strong><b>:</b></th>
                <span tal:content="llocConvocatoria"></span>
                <br/>
              </tal:omit-tag>
              <tal:omit-tag tal:condition="horaInici">
                <th style="display: inline-block;"><strong i18n:translate="">Dia/Hora inici</strong><b>:</b></th>
                <span tal:content="python:context.horaInici.strftime('%d/%m/%Y %H:%M')"></span>
                <br/>
              </tal:omit-tag>
              <tal:omit-tag tal:condition="horaFi">
                <th style="display: inline-block;"><strong i18n:translate="">Dia/Hora fi</strong><b>:</b></th>
                <span tal:content="python:context.horaFi.strftime('%d/%m/%Y %H:%M')"></span>
                <br/>
              </tal:omit-tag>
          </div>
        </div>
      </div>
    </div>

    <div class="container-fluid" style="border-bottom: 1px solid #b2b2b2;">
      <div class="row-fluid">
        <div class="span12" style="margin-bottom: 0px">
          <div class="row-fluid">
            <div class="span6">
              <tal:block tal:condition="membresConvocats">
                  <h4 i18n:translate=""> Attending members </h4>
                  <div tal:replace="structure membresConvocats"></div>
              </tal:block>
            </div>
            <div class="span6">
              <tal:block tal:condition="membresConvidats">
                  <h4 i18n:translate=""> Invited members </h4>
                  <span tal:replace="structure membresConvidats"></span>
              </tal:block>

              <tal:block tal:condition="llistaExcusats">
                  <h4 i18n:translate=""> Excused members </h4>
                  <span tal:replace="structure llistaExcusats"></span>
              </tal:block>

              <tal:block tal:condition="noAssistents">
                  <h4 i18n:translate=""> Missing members </h4>
                  <span tal:replace="structure noAssistents"></span>
              </tal:block>
            </div>
          </div>
        </div>
      </div>
    </div>

    <tal:block tal:condition="ordenDelDia">
      <h3 i18n:translate=""> Desenvolupament de la sessio </h3>
      <span tal:replace="structure ordenDelDia"></span>
    </tal:block>

    <tal:block tal:condition="signatura">
      <br/>
      <span tal:replace="structure signatura"></span>
    </tal:block>

</tal:block>

</html>

</tal:maindefines>
</metal:page>
