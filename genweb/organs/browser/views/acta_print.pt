
<tal:block i18n:domain="genweb.organs">




    <link href='//fonts.googleapis.com/css?family=Source+Sans+Pro:400,733,600' rel='stylesheet' type='text/css'>
    <link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />

    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap-theme.min.css">
    <link rel="stylesheet" href="++genwebupc++stylesheets/genwebupc.css">
    <link rel="stylesheet" media="print" href="++resource++genweb.organs.stylesheets/print.css">



    <img tal:condition="view/getActaLogo" tal:attributes="src view/getActaLogo" style="height:80px;" />
    <h1 style="text-align: left" tal:content="view/organGovernTitle">CONSELL DE GOVERN</h1>
    <div class="clearfix"></div>
    <hr/>

    <div class="container-fluid" style="border-bottom: 1px solid #b2b2b2;">
      <div class="row-fluid">
        <div class="span12" style="margin-bottom: 0px">
          <div class="row-fluid">
            <div class="span8">
              <tal:omit-tag tal:condition="context/Title">
                <th style="display: inline-block;"><strong i18n:translate="">Acta</strong><b>:</b></th>
                <span tal:content="context/Title"></span>
              </tal:omit-tag>
              <br/>
              <tal:omit-tag tal:condition="context/llocConvocatoria">
                <th style="display: inline-block;"><strong i18n:translate="">Session location</strong><b>:</b></th>
                <span tal:content="context/llocConvocatoria"></span>
              </tal:omit-tag>
            </div>
            <div class="span4">
              <tal:omit-tag tal:condition="context/dataSessio">
                <th style="display: inline-block;"><strong i18n:translate="">Sessio del dia</strong><b>:</b></th>
                <span tal:content="python:context.dataSessio.strftime('%d/%m/%Y')"></span>
              </tal:omit-tag>
              <br/>
              <tal:omit-tag tal:condition="context/horaInici">
                <th style="display: inline-block;"><strong i18n:translate="">Session start</strong><b>:</b></th>
                <span tal:content="python:context.horaInici.strftime('%H:%M')"></span>
              </tal:omit-tag>
              <br/>
              <tal:omit-tag tal:condition="context/horaFi">
                <th style="display: inline-block;"><strong i18n:translate="">Hora de fi</strong><b>:</b></th>
                <span tal:content="python:context.horaFi.strftime('%H:%M')"></span>
              </tal:omit-tag>
            </div>
          </div>
        </div>
      </div>
    </div>


    <div class="container-fluid" style="border-bottom: 1px solid #b2b2b2;">
      <div class="row-fluid">
        <div class="span12" style="margin-bottom: 0px">
          <div class="row-fluid">
            <div class="span6">
              <tal:block tal:condition="context/membresConvocats">
                  <h4 i18n:translate=""> Attending members </h4>
                  <div tal:replace="structure context/membresConvocats"/>
              </tal:block>
            </div>
            <div class="span6">
              <tal:block tal:condition="context/membresConvidats">
                  <h4 i18n:translate=""> Invited members </h4>
                  <span tal:replace="structure context/membresConvidats" />
              </tal:block>

              <tal:block tal:condition="context/llistaExcusats">
                  <h4 i18n:translate=""> Excused members </h4>
                  <span tal:replace="structure context/llistaExcusats"/>
              </tal:block>

              <tal:block tal:condition="context/noAssistents">
                  <h4 i18n:translate=""> Missing members </h4>
                  <span tal:replace="structure context/noAssistents"/>
              </tal:block>
            </div>
            </div>
            </div></div></div>


    <tal:block tal:condition="context/ordenDelDia">
        <h3 i18n:translate=""> Acta Body </h3>
        <span tal:replace="structure context/ordenDelDia" />
    </tal:block>

    <tal:block tal:condition="context/signatura">
      <br/>
        <span tal:replace="structure context/signatura" />
    </tal:block>


</tal:block>
