<?xml version="1.0" encoding="utf-8" ?>
<registry>

    <records
        interface="Products.CMFPlone.interfaces.IBundleRegistry"
        prefix="plone.bundles/organs-bundle">

        <value key="enabled">True</value>
        <value key="compile">True</value>
        <value key="resources">
            <element>organs-main</element>
        </value>
        <value key="jscompilation" />
        <value key="csscompilation">++theme++genweb.organs/stylesheets/theme-organs.min.css</value>

    </records>

    <records
        interface="genweb.organs.controlpanel.IOrgansSettings"
        purge="False"/>

    <records
        interface="genweb.organs.firma_documental.controlpanel.IFirmaDocumentalSettings"
        purge="False"/>

</registry>
